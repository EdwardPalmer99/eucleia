import <math>

func DifferenceSumOfSquaresAndSquareOfSum(int max)
{
    int sum = 0;
    int sumSquares = 0;

    for (int i = 1; i <= max; ++i)
    {
        sum = sum + i;
        sumSquares = sumSquares + pow(i, 2);   // Careful! Issues with passing Int to func expecting float!
    }

    int lhs = pow(sum, 2);    // Note the implicit conversion back.
    int rhs = sumSquares;

    return (lhs - rhs);
}

DifferenceSumOfSquaresAndSquareOfSum(100); // 25164150
